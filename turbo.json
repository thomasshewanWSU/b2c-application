{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "globalEnv": [
    "PASSWORD",
    "VITEST",
    "E2E",
    "TEST_DATABASE_URL",
    "DATABASE_URL",
    "JWT_SECRET"
  ],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "dependsOn": ["check-types"]
    },
    "dev:test": {
      "dependsOn": ["check-types"]
    },
    "test-1": {},
    "dev:test-1": {
      "dependsOn": ["dev:test"]
    },
    "test-2": {},
    "dev:test-2": {
      "dependsOn": ["dev:test"]
    },
    "test-3": {},
    "dev:test-3": {
      "dependsOn": ["dev:test"]
    },
    "all:test": {
      "dependsOn": ["test", "test-1", "test-2", "test-3"]
    },
    "db:generate": {
      "dependsOn": ["^db:generate"]
    }
  },
  "envMode": "loose"
}
